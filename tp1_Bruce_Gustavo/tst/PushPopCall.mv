;;;;;;;;;;; Programa que testa várias operações aritméticas e de bitwise ;;;;;;;;;;;;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

LOAD R0 const1
LOAD R1 const10 
PUSH R0
CALL func1
PUSH R1
CALL func1
HALT

func1: POP R2
WRITE R2
RET

const1: WORD 1
const10: WORD 10
END
